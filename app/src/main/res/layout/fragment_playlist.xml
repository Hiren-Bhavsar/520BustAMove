<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#BABABA" >

    <TextView
        android:id="@+id/textView11"
        android:layout_width="143dp"
        android:layout_height="32dp"
        android:layout_marginStart="240dp"
        android:layout_marginTop="440dp"
        android:text="IN VEHICLE"
        android:textAlignment="center"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageButton11" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="145dp"
        android:layout_height="28dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="440dp"
        android:text="CYCLING"
        android:textAlignment="center"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageButton11" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="144dp"
        android:layout_height="29dp"
        android:layout_marginStart="240dp"
        android:layout_marginTop="212dp"
        android:text="RUNNING"
        android:textAlignment="center"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageButton11" />

    <ImageButton
        android:id="@+id/imageButton10"
        android:layout_width="141dp"
        android:layout_height="138dp"
        android:layout_marginTop="275dp"
        android:layout_marginEnd="30dp"
        android:background="#00FFFFFF"
        android:contentDescription="person running"
        android:scaleType="centerCrop"
        android:visibility="visible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/running" />

    <ImageButton
        android:id="@+id/imageButton11"
        android:layout_width="141dp"
        android:layout_height="138dp"
        android:layout_marginStart="132dp"
        android:layout_marginTop="60dp"
        android:background="#00FFFFFF"
        android:scaleType="centerCrop"
        android:visibility="visible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/still" />

    <ImageButton
        android:id="@+id/imageButton12"
        android:layout_width="141dp"
        android:layout_height="138dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="275dp"
        android:background="#00FFFFFF"
        android:scaleType="centerCrop"
        android:visibility="visible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/walking" />

    <ImageButton
        android:id="@+id/imageButton5"
        android:layout_width="141dp"
        android:layout_height="138dp"
        android:layout_marginTop="500dp"
        android:layout_marginEnd="30dp"
        android:background="#00FFFFFF"
        android:scaleType="centerCrop"
        android:visibility="visible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/vehicle" />

    <ImageButton
        android:id="@+id/imageButton6"
        android:layout_width="141dp"
        android:layout_height="138dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="500dp"
        android:background="#00FFFFFF"
        android:scaleType="centerCrop"
        android:visibility="visible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/cycling" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="146dp"
        android:layout_height="28dp"
        android:layout_marginStart="132dp"
        android:text="STILL"
        android:textAlignment="center"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageButton11" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="144dp"
        android:layout_height="29dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="212dp"
        android:text="WALKING"
        android:textAlignment="center"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageButton11" />
</androidx.constraintlayout.widget.ConstraintLayout>